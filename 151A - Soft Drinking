// note : you can use the standard library , but it's not make sence to use it in this case

#include <iostream>
#include <algorithm>  
int main() {
    unsigned int n=0, k=0, l=0, c=0, d=0, p=0, nl=0, np=0;
    std::cin>>n>>k>>l>>c>>d>>p>>nl>>np;
    
    if(n>0 && n<=1000 &&  k>0 && k<=1000 && l>0 && l<=1000 && c>0 && c<=1000 && d>0 && d<=1000 && p>0 && p<=1000 &&nl>0 && nl<=1000 &&np>0 && np<=1000 ){
        
        std::cout<<(std::min(std::min((k*l)/nl ,c*d ) , p / np))/n<<std::endl;
    }

    return 0;
}
